        <Label Content="Apellido:" HorizontalAlignment="Left" Margin="75,210,0,0" VerticalAlignment="Top"/>
        <Label Content="Teléfono:" HorizontalAlignment="Left" Margin="73,241,0,0" VerticalAlignment="Top"/>
        <Label Content="Correo:" HorizontalAlignment="Left" Margin="83,272,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtRutCliente" HorizontalAlignment="Left" Height="23" Margin="136,147,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
